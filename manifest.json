{
  "manifest_version": 3,
  "name": "Domain Changer",
  "description": "Simple Domain Changer",
  "version": "1.0",
  "action": {
    "default_icon": {
      "16": "images/icon16.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}